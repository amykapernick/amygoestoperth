{
  "name": "amy-goes-to-perth",
  "version": "2.0.0",
  "description": "Amy Goes to Perth blog",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amykapernick/amygoestoperth.git"
  },
  "scripts": {
    "start": "npm run blogs && npm run develop",
    "develop": "gatsby develop",
    "build": "npm run blogs && gatsby build",
    "serve": "gatsby serve",
    "tests": "npm run a11y",
    "a11y": "pa11y --reporter cli http://localhost:8000 > a11yreport.txt",
    "clean": "rm -rf .cache/ public/",
    "blogImages": "cp src/blog/img/* static/img/blog/",
    "updatePosts": "git submodule update --remote",
    "blogs": "npm run updatePosts && npm run blogImages"
  },
  "author": "AimHigher Web Design",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amykapernick/amygoestoperth/issues"
  },
  "homepage": "https://github.com/amykapernick/amygoestoperth#readme",
  "dependencies": {
    "@raae/gatsby-remark-oembed": "^0.0.5",
    "dotenv": "^8.1.0",
    "gatsby": "^2.14.0",
    "gatsby-image": "^2.2.14",
    "gatsby-plugin-google-tagmanager": "^2.0.5",
    "gatsby-plugin-netlify-cms": "^3.0.16",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "gatsby-plugin-sass": "^2.1.12",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.3",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.2.12",
    "netlify-cms": "^2.1.1",
    "node-sass": "^4.12.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-feather": "^1.1.3",
    "react-helmet": "^5.2.0"
  }
}
